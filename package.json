    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.5",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.56.9",
    "cache-manager": "^7.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.13",
    "ejs": "^3.1.10",
    "express-basic-auth": "^1.2.1",
